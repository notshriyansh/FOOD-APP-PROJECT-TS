{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "esModuleInterop": true
  },
  "include": ["src"],
  "exclude": ["dist", "build", "node_modules"]
}
